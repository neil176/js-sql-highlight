{
  "name": "js-sql-highlight",
  "displayName": "js-sql-highlight",
  "description": "Syntax highlighting for SQL in JS strings",
  "version": "1.0.0",
  "publisher": "neil176",
  "icon": "",
  "engines": {
    "vscode": "^1.19.0"
  },
  "galleryBanner": {
    "color": "#dedede",
    "theme": "light"
  },
  "repository": {
    "url": "https://github.com/neil176/js-sql-highlight"
  },
  "bugs": {
    "url": "https://github.com/neil176/js-sql-highlight/issues"
  },
  "categories": [],
  "keywords": [
    "javascript",
    "sql"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.sql"
        ],
        "scopeName": "sql-string.injection",
        "path": "./js-inline-sql.json",
        "embeddedLanguages": {
          "meta.embedded.*": "sql"
        }
      }
    ]
  }
}
